<launch>
	<arg name="rviz" default="false" />
    <arg name="voxel_size" default="1.0" />

    <param name="voxel_size" type="double" value="$(arg voxel_size)" />
    <param name="poses_limit" type="int" value="150" /> <!-- Poses limit for initial voxel_size = 1 -->

	<param name="voxel_size" type="double" value="0.5" />
	<param name="file_path" type="string" value="/root/share/dataset/NewerCollege/underground_mine/bag/underground_easy.bag" />
        <param name="trajectory_output_path" type="string" value="$(find balm2)/output_nc/balm_underground_easy.tum" />
	<node pkg="balm2" type="benchmark_realworld" name="benchmark_realworld" output="screen" />

	<group if="$(arg rviz)">
		<node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find balm2)/rviz_cfg/back.rviz" />
	</group>  

</launch>
